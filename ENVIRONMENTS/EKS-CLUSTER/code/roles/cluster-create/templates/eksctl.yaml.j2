apiVersion: eksctl.io/v1alpha5
kind: ClusterConfig

metadata:
  name: "{{ClusterName}}"
  region: "{{apenvironment.region}}"

cloudWatch:
  clusterLogging:
    enableTypes: ["*"]
  
vpc:
  subnets:
    private:
      "{{PrivateSubnetAZ1}}":
        id: "{{PrivateSubnetId1}}"
      "{{PrivateSubnetAZ2}}":
        id: "{{PrivateSubnetId2}}"
    public:
      "{{PublicSubnetAZ1}}":
        id: "{{PublicSubnetId1}}"
      "{{PublicSubnetAZ2}}":
        id: "{{PublicSubnetId2}}"
        
managedNodeGroups:
  "{{managedndgrps}}"

iam:
  withOIDC: true